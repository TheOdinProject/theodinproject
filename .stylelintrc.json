{
  "extends": ["stylelint-config-standard"],
  "rules": {
    "selector-class-pattern": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "utility",
          "theme"
        ]
      }
    ],
    "at-rule-no-deprecated": [
      true,
      {
        "ignoreAtRules": [
          "apply"
        ]
      }
    ]
  }
}
